<h1>Invite others!</h1>
<div>
  <%= bootstrap_form_for :users, url: invite_users_path do |f| %>
  <%= f.text_field :email %>
</div>
<div>
  <%= f.radio_button :rank, 'Admin' %> Admin
  <%= f.radio_button :rank, 'Mod' %> Mod
</div>

<div class="form-group">
  <% @subreddit.each do |saiddit| %>
  <%= f.select :board_control, "#{saiddit.name}" %> <%= saiddit.name %>
  <% end %>
</div>

<%= f.submit %>
<% end %>
